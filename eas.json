{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "APP_ENV": ".env.development" }
    },
    "production": {
      "releaseChannel": "production",
      "env": { "APP_ENV": ".env.production" }
    },
    "preview": {
      "distribution": "internal",
      "env": { "APP_ENV": ".env.production" }
    }
  },
  "submit": {
    "production": {}
  }
}
